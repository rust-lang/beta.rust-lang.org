<section id="cli-use-it" class="red">
    <div class="w-100 mw-none ph3 mw8-m mw9-l center f3">
        <header>
            <h2>{{text get-started-exclamation}}</h2>
            <div class="highlight"></div>
        </header>

        <div>
            <p>{{text cli-example-description}}</p>
        </div>

        <div class="flex-l">
            <article class="w-50-l mr4-l">
                <h3>{{text cli-example-inputs-heading}}</h3>
                <pre><code class="block">{{> components/what/cli/code-inputs }}</code></pre>
            </article>

            <article class="w-50-l ml4-l mt5 mt0-l">
                <h3>{{text cli-example-main-heading}}</h3>
                <pre><code class="block">{{> components/what/cli/code-main }}</code></pre>
            </article>
        </div>

        <a href="https://rust-lang-nursery.github.io/cli-wg/" class="button button-secondary">{{text cli-example-link}}</a>
    </div>
</section>
